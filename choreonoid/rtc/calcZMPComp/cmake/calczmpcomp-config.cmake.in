# calcZMPComp CMake config file
#
# This file sets the following variables:
# calcZMPComp_FOUND - Always TRUE.
# calcZMPComp_INCLUDE_DIRS - Directories containing the calcZMPComp include files.
# calcZMPComp_IDL_DIRS - Directories containing the calcZMPComp IDL files.
# calcZMPComp_LIBRARIES - Libraries needed to use calcZMPComp.
# calcZMPComp_DEFINITIONS - Compiler flags for calcZMPComp.
# calcZMPComp_VERSION - The version of calcZMPComp found.
# calcZMPComp_VERSION_MAJOR - The major version of calcZMPComp found.
# calcZMPComp_VERSION_MINOR - The minor version of calcZMPComp found.
# calcZMPComp_VERSION_REVISION - The revision version of calcZMPComp found.
# calcZMPComp_VERSION_CANDIDATE - The candidate version of calcZMPComp found.

message(STATUS "Found calcZMPComp-@PROJECT_VERSION@")
set(calcZMPComp_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(calcZMPComp_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(calcZMPComp_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(calcZMPComp_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(calcZMPComp_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(calcZMPComp_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(calcZMPComp_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(calcZMPComp_DEFINITIONS ${<dependency>_DEFINITIONS})

set(calcZMPComp_VERSION @PROJECT_VERSION@)
set(calcZMPComp_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(calcZMPComp_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(calcZMPComp_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(calcZMPComp_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)


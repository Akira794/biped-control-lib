# FootController CMake config file
#
# This file sets the following variables:
# FootController_FOUND - Always TRUE.
# FootController_INCLUDE_DIRS - Directories containing the FootController include files.
# FootController_IDL_DIRS - Directories containing the FootController IDL files.
# FootController_LIBRARIES - Libraries needed to use FootController.
# FootController_DEFINITIONS - Compiler flags for FootController.
# FootController_VERSION - The version of FootController found.
# FootController_VERSION_MAJOR - The major version of FootController found.
# FootController_VERSION_MINOR - The minor version of FootController found.
# FootController_VERSION_REVISION - The revision version of FootController found.
# FootController_VERSION_CANDIDATE - The candidate version of FootController found.

message(STATUS "Found FootController-@PROJECT_VERSION@")
set(FootController_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(FootController_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(FootController_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(FootController_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(FootController_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(FootController_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(FootController_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(FootController_DEFINITIONS ${<dependency>_DEFINITIONS})

set(FootController_VERSION @PROJECT_VERSION@)
set(FootController_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(FootController_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(FootController_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(FootController_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

